<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="admin/contact.xml"/>
        <import resource="front/contact.xml"/>
    </imports>

    <services>
        <service id="contact.manager" class="WellCommerce\Bundle\ContactBundle\Manager\ContactManager" parent="core.manager">
            <argument type="service" id="contact.repository"/>
            <argument type="service" id="contact.factory"/>
        </service>
    </services>
</container>
